{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$components/*": [
				"src/components/*"
			],
			"$static/*": [
				"src/static/*"
			],
			"$images/*": [
				"src/static/images/*"
			],
			"$fonts/*": [
				"src/static/fonts/*"
			]
		}
	},
	"resolveJsonModule": true,
	"esModuleInterop": true,
}